{
  "name": "zlibt",
  "version": "0.1.1",
  "description": "typescript zlib",
  "main": "dist/zlibt.js",
  "bin": "bin/bin.js",
  "types": "zlib_and_gzip.min.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch src --delay 1 --exec ts-node src/zlibtest.ts",
    "build:prod": "webpack --config config/prod/config.js",
    "build:gzip:prod": "webpack --config config/prod/config.gzip.js",
    "build:zip:prod": "webpack --config config/prod/config.zip.js",
    "build:zlib:prod": "webpack --config config/prod/config.zlib.js",
    "build:raw:prod": "webpack --config config/prod/config.raw.js",
    "build:dev":"npm run clear:dev & webpack --config config/dev/dev.config.js --watch",
    "build:gzip:dev": "npm run clear:dev & webpack --config config/dev/dev.config.gzip.js --watch",
    "build:zip:dev": "npm run clear:dev & webpack --config config/dev/dev.config.zip.js --watch",
    "build:zlib:dev": "npm run clear:dev & webpack --config config/dev/dev.config.zlib.js --watch",
    "build:raw:dev": "npm run clear:dev & webpack --config config/dev/dev.config.raw.js --watch",
    "clear:dev":"del-cli --force ./dist/dev",
    "clear:prod":"del-cli --force ./dist/prod",
    "lint": "tslint --project tsconfig.json src/*",
    "lint:fix": "tslint --project tsconfig.json src/* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadfishDT/zlibt.git"
  },
  "keywords": [
    "zlib",
    "gzip",
    "zip",
    "js",
    "typescript"
  ],
  "author": "MadfishDT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MadfishDT/zlibt/issues"
  },
  "homepage": "https://madfishdt.github.io",
  "devDependencies": {
    "codelyzer": "^6.0.1",
    "del-cli": "^3.0.1",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^7.12.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {}
}
